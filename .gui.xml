            <gc:label colspan="2" id="277" name="LABEL 277" x="21" y="12">
                <align>top left</align>
                <background>#01000000</background>
                <binding/>
                <colour>#80808080</colour>
                <shadow>1</shadow>
                <text>Body:</text>
                <visible>1</visible>
            </gc:label>
        </gc:controls>
    </gc:guilayout>
</gui>
