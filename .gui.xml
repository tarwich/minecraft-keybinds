<gui xmlns:gb="http://eq2online.net/macros/guibinding" xmlns:gc="http://eq2online.net/macros/guiconfiguration">
    <!-- GUI slot bindings -->
    <gb:bindings>
        <gb:binding slot="playback">default</gb:binding>
        <gb:binding slot="scoreboard">ingame</gb:binding>
        <gb:binding slot="indebug">ingame</gb:binding>
        <gb:binding slot="inchat">inchat</gb:binding>
        <gb:binding slot="ingame">ingame</gb:binding>
    </gb:bindings>
    <!-- Config for layout 'default' -->
    <gc:guilayout columns="6" display="default" dots="false" grid="false" name="default" padding="2" rows="10">
        <gc:controls>
            <gc:button id="258" name="BUTTON 258" x="0" y="9">
                <background>#B0000000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>0</sticky>
                <text>Craft</text>
                <visible>1</visible>
            </gc:button>
            <gc:button id="259" name="btnAttack" x="4" y="1">
                <background>#80000000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>1</sticky>
                <text>Attack</text>
                <visible>1</visible>
            </gc:button>
            <gc:label id="260" name="lblChat" x="2" y="2">
                <align>top left</align>
                <background>#AA000000</background>
                <binding/>
                <colour>#00FF00</colour>
                <shadow>1</shadow>
                <text>ERROR</text>
                <visible>1</visible>
            </gc:label>
            <gc:button id="261" name="btnAFK" x="4" y="4">
                <background>#80008000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>1</sticky>
                <text>Autoreply</text>
                <visible>1</visible>
            </gc:button>
            <gc:button id="262" name="btnAntiIdle" x="4" y="3">
                <background>#80008000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>1</sticky>
                <text>Anti Idle</text>
                <visible>1</visible>
            </gc:button>
            <gc:button id="263" name="btnPace" x="4" y="5">
                <background>#B0000000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>1</sticky>
                <text>Pace</text>
                <visible>1</visible>
            </gc:button>
            <gc:button id="278" name="BUTTON 278" x="2" y="3">
                <background>#B0000000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>0</sticky>
                <text>Find  Item</text>
                <visible>1</visible>
            </gc:button>
            <gc:button id="279" name="btnAutoplant" x="4" y="2">
                <background>#80800000</background>
                <colour>#00FF00</colour>
                <hide>0</hide>
                <sticky>1</sticky>
                <text>Autoplant</text>
                <visible>1</visible>
            </gc:button>
        </gc:controls>
    </gc:guilayout>
    <!-- Config for layout 'inchat' -->
    <gc:guilayout columns="6" display="inchat" dots="false" grid="false" name="inchat" padding="2" rows="10">
        <gc:controls/>
    </gc:guilayout>
    <!-- Config for layout 'ingame' -->
    <gc:guilayout columns="25" display="ingame" dots="false" grid="false" name="ingame" padding="1" rows="15">
        <gc:controls>
            <gc:textarea colspan="17" id="256" name="debug" rowspan="7" x="8" y="2">
                <colour>FF00FF00</colour>
                <lifespan>200</lifespan>
                <visible>1</visible>
            </gc:textarea>
            <gc:label colspan="6" id="257" name="lblAuctionIngame" rowspan="2" x="0" y="3">
                <align>top left</align>
                <background>#AA000000</background>
                <binding/>
                <colour>#00FF00</colour>
                <shadow>1</shadow>
                <text>56 Obsidian\n$0 + $100 (0s)</text>
                <visible>0</visible>
            </gc:label>
            <gc:label colspan="3" id="264" name="LABEL 264" x="0" y="1">
                <align>top left</align>
                <background>#01000000</background>
                <binding>light: %LIGHT%</binding>
                <colour>#00FF00</colour>
                <shadow>1</shadow>
                <text>%%</text>
                <visible>1</visible>
            </gc:label>
            <gc:label colspan="3" id="265" name="LABEL 265" x="0" y="2">
